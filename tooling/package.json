{
  "name": "@skyscout/tooling",
  "version": "1.0.0",
  "description": "SkyScout AI Enhanced Development Tooling",
  "main": "setup-enhanced-tooling.js",
  "bin": {
    "skyscout-tooling": "./cli.js",
    "skyscout": "./cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx cli.ts",
    "start:enhanced": "tsx setup-enhanced-tooling.ts",
    "validate": "tsx cli.ts validate",
    "analyze": "tsx cli.ts analyze",
    "generate:component": "tsx cli.ts generate component",
    "generate:service": "tsx cli.ts generate service",
    "generate:hook": "tsx cli.ts generate hook",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "tooling",
    "development",
    "architecture",
    "performance",
    "code-generation",
    "clean-architecture",
    "typescript",
    "react"
  ],
  "author": "SkyScout AI Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.10.0",
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skyscout-ai/skyscout-ai.git",
    "directory": "tooling"
  },
  "files": [
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts",
    "cli.js",
    "setup-enhanced-tooling.js"
  ]
}
