{
  "name": "@skyscout/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "perf:lighthouse": "node ../../scripts/performance-monitor.js lighthouse",
    "perf:bundle": "node ../../scripts/performance-monitor.js bundle",
    "perf:full": "node ../../scripts/performance-monitor.js full",
    "perf:monitor": "npm run build && npm run perf:full",
    "perf:ci": "npm run build && node ../../scripts/performance-monitor.js full",
    "bundle:stats": "npx webpack-bundle-analyzer .next/analyze/client.html",
    "bundle:size-limit": "npx size-limit",
    "speed-test": "npm run build && npm run start & sleep 5 && npm run perf:lighthouse && kill %1"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@skyscout/ui": "*",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.302.0",
    "mapbox-gl": "^3.13.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.7",
    "recharts": "^2.8.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.3",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
