{
  "name": "@skyscout/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "dev:fast": "cross-env NODE_ENV=development SKIP_TYPE_CHECK=true SKIP_LINT=true next dev",
    "dev:turbo": "next dev --turbo",
    "dev:no-turbo": "DISABLE_TURBO=true next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:fast": "cross-env SKIP_TYPE_CHECK=true SKIP_LINT=true next build",
    "build:analyze": "ANALYZE=true next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "build:profile": "PROFILE=true next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate:integration": "node scripts/validate-lib-integration.js",
    "validate:imports": "eslint . --rule 'no-restricted-imports: [\"error\", {\"patterns\": [\"../../../libs/*\"]}]'",
    "validate:architecture": "npm run type-check && npm run validate:imports && npm run validate:integration",
    "check:turbo": "node scripts/check-turbo-compatibility.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test --grep @visual",
    "analyze": "ANALYZE=true npm run build && npm run bundle:stats",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "bundle:stats": "npx webpack-bundle-analyzer .next/analyze/client-stats.json -p 8888 -O",
    "bundle:size-limit": "npx size-limit",
    "bundle:check": "npm run build && npm run bundle:size-limit",
    "bundle:analyze": "node scripts/bundle-analysis.js",
    "bundle:trace": "TRACE=true npm run build",
    "perf:lighthouse": "node ../../scripts/performance-monitor.js lighthouse",
    "perf:lighthouse:ci": "lhci autorun",
    "perf:bundle": "node ../../scripts/performance-monitor.js bundle",
    "perf:full": "node ../../scripts/performance-monitor.js full",
    "perf:monitor": "npm run build && npm run perf:full",
    "perf:ci": "npm run build && node ../../scripts/performance-monitor.js full",
    "perf:web-vitals": "node scripts/web-vitals-monitor.js",
    "perf:memory": "node --expose-gc scripts/memory-test.js",
    "monitor:runtime": "node scripts/runtime-monitor.js",
    "monitor:errors": "node scripts/error-tracking.js",
    "monitor:metrics": "node scripts/metrics-collector.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:deps": "audit-ci --moderate",
    "security:snyk": "snyk test",
    "validate": "run-p type-check lint test:coverage bundle:check security:audit",
    "ci": "run-s validate perf:lighthouse:ci",
    "preprod": "run-s validate build perf:ci",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:deps": "rm -rf node_modules && npm install",
    "reset": "npm run clean && npm run clean:deps"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-web": "^0.40.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@sentry/nextjs": "^8.30.0",
    "@skyscout/ui": "*",
    "@tanstack/react-query": "^5.56.2",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^11.4.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "glob": "^11.0.3",
    "lucide-react": "^0.302.0",
    "mapbox-gl": "^2.15.0",
    "next": "^14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-intersection-observer": "^9.13.0",
    "react-map-gl": "^7.1.7",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.8",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.2.0",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@lhci/cli": "^0.14.0",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.47.0",
    "@size-limit/preset-big-lib": "^11.2.0",
    "@size-limit/webpack": "^11.2.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/mapbox-gl": "^2.7.19",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.16",
    "bundle-stats-webpack-plugin": "^4.14.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.2.0",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.32",
    "puppeteer": "^23.4.0",
    "size-limit": "^11.2.0",
    "snyk": "^1.1293.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-stats-plugin": "^1.1.3"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false
}
